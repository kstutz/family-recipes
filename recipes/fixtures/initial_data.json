  {
    "model": "recipes.instruction",
    "pk": 1,
    "fields": {
      "step": 1,
      "text": "schälen, kleinschneiden"
    }
  },
  {
    "model": "recipes.instruction",
    "pk": 2,
    "fields": {
      "step": 1,
      "text": "im größten Topf mit viel Butter anbraten"
    }
  },
  {
    "model": "recipes.instruction",
    "pk": 3,
    "fields": {
      "step": 2,
      "text": "schälen (Hokkaido nicht), in dünne Streifen schneiden"
    }
  },
  {
    "model": "recipes.instruction",
    "pk": 4,
    "fields": {
      "step": 2,
      "text": "zur Zwiebel geben, kurz anbraten"
    }
  },
  {
    "model": "recipes.instruction",
    "pk": 5,
    "fields": {
      "step": 2,
      "text": "mit Wasser auffüllen"
    }
  },
  {
    "model": "recipes.instruction",
    "pk": 6,
    "fields": {
      "step": 2,
      "text": "20 min kochen"
    }
  },
  {
    "model": "recipes.instruction",
    "pk": 7,
    "fields": {
      "step": 2,
      "text": "würzen mit Salz, Pfeffer, Zucker, Muskat"
    }
  },
  {
    "model": "recipes.instruction",
    "pk": 8,
    "fields": {
      "step": 3,
      "text": "zuletzt reinrühren"
    }
  },
  {
    "model": "recipes.instruction",
    "pk": 9,
    "fields": {
      "step": 4,
      "text": "schälen, kleinschneiden, anbraten"
    }
  },
  {
    "model": "recipes.instruction",
    "pk": 10,
    "fields": {
      "step": 5,
      "text": "zusammenschütten, umrühren"
    }
  },
  {
    "model": "recipes.instruction",
    "pk": 11,
    "fields": {
      "step": 5,
      "text": "20 min köcheln"
    }
  },
  {
    "model": "recipes.instruction",
    "pk": 12,
    "fields": {
      "step": 5,
      "text": "mit Salz, Chili, Ras el Hanout würzen"
    }
  },
  {
    "model": "recipes.instruction",
    "pk": 13,
    "fields": {
      "step": 5,
      "text": "evtl. Orangensaft oder getrocknete Tomaten reinrühren"
    }
  },
  {
    "model": "recipes.instruction",
    "pk": 14,
    "fields": {
      "step": 6,
      "text": "Mehl in der Teigschüssel abwiegen"
    }
  },
  {
    "model": "recipes.instruction",
    "pk": 15,
    "fields": {
      "step": 6,
      "text": "Milch langsam zugeben, dabei mit dem Metall-Schneebesen immer rühren, bis Milch komplett vom Mehl aufgenommen worden ist"
    }
  },
  {
    "model": "recipes.instruction",
    "pk": 16,
    "fields": {
      "step": 6,
      "text": "so lange Milch reinrühren, bis ein dünnflüssiger Brei entstanden ist (braucht wahrscheinlich mehr Milch)"
    }
  },
  {
    "model": "recipes.instruction",
    "pk": 17,
    "fields": {
      "step": 7,
      "text": "Eier aufschlagen und reinrühren"
    }
  },
  {
    "model": "recipes.instruction",
    "pk": 18,
    "fields": {
      "step": 7,
      "text": "evtl. mehr Milch zugeben, wenn jetzt wieder zu dickflüssig"
    }
  },
  {
    "model": "recipes.instruction",
    "pk": 19,
    "fields": {
      "step": 8,
      "text": "Zucker und 1 Prise Salz reinrühren"
    }
  },
  {
    "model": "recipes.instruction",
    "pk": 20,
    "fields": {
      "step": 9,
      "text": "Öl in Pfanne erhitzen, mit Küchentuch in Pfanne verteilen"
    }
  },
  {
    "model": "recipes.instruction",
    "pk": 21,
    "fields": {
      "step": 9,
      "text": "Teig mit Suppenkelle in Pfanne geben, verteilen"
    }
  },
  {
    "model": "recipes.instruction",
    "pk": 22,
    "fields": {
      "step": 9,
      "text": "von beiden Seiten braun backen, zwischendrin mit Pfannenwender wenden"
    }
  },
